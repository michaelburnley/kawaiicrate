 <header class="navbar navbar-fixed-top navbar-inverse">
	<div class="container">
			<%= link_to "Kawaii Crate", root_path, id: "logo" %>
		<nav>
			<ul class="nav navbar-nav navbar-right">
				<li><%= link_to "Subscriptions", plans_path %></li>
				<li><%= link_to "Past Crates", past_path %></li>
				<li><%= link_to "FAQ", faq_path %></li>
				<% if logged_in? %>
					<li class="dropdown">
						<a href="#" class="dropdown-toggle" data-toggle="dropdown">Account</a>
						<ul class="dropdown-menu">
							<li><%= link_to "Profile", current_user %></li>
							<li><%= link_to "Settings", edit_user_path(current_user) %></li>
							<li class="divider"></li>
							<li><%= link_to "Log out", logout_path, method: "delete" %></li>	
						</ul>
					</li>
				<% else %>
				<li><%= link_to "Log In", login_path %></li>
				<li style="padding-top: 8px;"><%= button_to "SUBSCRIBE", signup_path, :class => "btn btn-primary", :method => :get %></li>
				<% end %>
			</ul>
		</nav>
	</div>
 </header>
